{
  "expo": {
    "name": "react-native-convex-demo",
    "slug": "react-native-convex-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tomhaddad.react-native-convex-demo"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.tomhaddad.react-native-convex-demo"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-secure-store"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e337aa36-48ed-4ae7-b977-202707a95265"
      }
    },
    "owner": "tomhaddad"
  }
}
